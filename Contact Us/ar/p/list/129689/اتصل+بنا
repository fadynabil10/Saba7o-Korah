		<script type="text/javascript">
		$(document).ready(function(){
            $('.AccordTitle').next().slideToggle();
            $('.AccordTitle a').live('click',function(e){
                e.preventDefault();
                $(this).find('i').toggleClass('icon-caret-down');
                $(this).find('i').toggleClass('icon-caret-up');
                $(this).parent().next().slideToggle();
            });
        });
        function popup_share(url, width, height)
        {
            day = new Date();
            id = day.getTime();
            eval("page" + id + " = window.open(url, '" + id + "', 'toolbar=0,scrollbars=1,location=1,statusbar=0,menubar=0,resizable=0,width=" + width + ", height=" + height + ", left = 363, top = 144');");
        }
		</script>
