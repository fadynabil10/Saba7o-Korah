            $(document).ready(function(){
                $.stellar({
                    horizontalScrolling: false,
                    verticalOffset: 40
                });
                $(window).scroll(function(){
                    if($(this).scrollTop() >= ($(".Header").height() + $(".PreHeader").height() + 20) && $(window).width() > 969) {
